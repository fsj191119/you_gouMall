/**app.wxss**/
@import "./style/iconfont.wxss";


button::after{
border:none;
}

 button {
  border-radius:0;
  background-color: #fff;
}

page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  --themeColor:#890411;
  font-size: 32rpx
}
image{
  width: 100%;
}


